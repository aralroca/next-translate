{
  "name": "i18n-next-static",
  "version": "0.1.0",
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "husky": "3.1.0",
    "jest": "24.9.0",
    "babel-jest": "24.9.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.2",
    "@testing-library/react": "9.3.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn test"
    }
  },
  "jest": {
    "roots": ["<rootDir>/lib"],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  }
}
